        <item name="G">--·</item>
        <item name="H">····</item>
        <item name="I">··</item>
        <item name="J">·---</item>
        <item name="K">-·-</item>
        <item name="L">·-··</item>
        <item name="M">--</item>
        <item name="N">-·</item>
        <item name="O">---</item>
        <item name="P">·--·</item>
        <item name="Q">--·-</item>
        <item name="R">·-·</item>
        <item name="S">···</item>
